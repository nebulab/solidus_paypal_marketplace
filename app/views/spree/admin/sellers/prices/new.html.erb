<% admin_breadcrumb(link_to plural_resource_name(Spree::Price), spree.admin_prices_path) %>
<% admin_breadcrumb(t('spree.new_price')) %>

<%= render partial: 'spree/shared/error_messages', locals: { target: @seller } %>

<%= form_for [:admin, @price] do |f| %>
  <fieldset data-hook="new_price">
    <legend align="center"><%= t('spree.new_price') %></legend>

    <%= render 'form', f: f %>

    <%= render partial: 'spree/admin/shared/new_resource_links' %>
  </fieldset>
<% end %>
