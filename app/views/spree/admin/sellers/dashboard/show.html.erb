<% case @seller.status %>
<% when 'accepted' %>
  Accepted
<% when 'rejected' %>
  Rejected
<% when 'pending' %>
  <% if @seller.action_url.present? %>
    <%= render 'paypal_onboarding_link' %>
  <% else %>
    Contact the platform owner
  <% end %>
<% else %>
  Contact the platform owner
<% end %>
